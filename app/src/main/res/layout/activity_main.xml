<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/home"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_historia"
        android:layout_width="281dp"
        android:layout_height="52dp"
        android:backgroundTint="#00DF1414"
        android:onClick="historia"
        android:text="a"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button_mapa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.902"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_mapa"
        android:layout_width="281dp"
        android:layout_height="52dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00EC1414"
        android:onClick="mapa"
        android:text="b"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button_locais"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_locais"
        android:layout_width="281dp"
        android:layout_height="52dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="#00CD2828"
        android:onClick="locais"
        android:text="c"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button_eventos"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_eventos"
        android:layout_width="281dp"
        android:layout_height="52dp"
        android:layout_marginBottom="20dp"
        android:backgroundTint="#00DC0F0F"
        android:onClick="eventos"
        android:text="d"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button_musicas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_musicas"
        android:layout_width="281dp"
        android:layout_height="52dp"
        android:layout_marginBottom="48dp"
        android:backgroundTint="#00EA0B0B"
        android:onClick="musicas"
        android:text="e"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toTopOf="@+id/button_desenvolvedores"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/button_desenvolvedores"
        android:layout_width="154dp"
        android:layout_height="48dp"
        android:layout_marginBottom="24dp"
        android:backgroundTint="#00EA0B0B"
        android:onClick="desenvolvedores"
        android:text="f"
        android:textColor="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/imageView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />


</androidx.constraintlayout.widget.ConstraintLayout>